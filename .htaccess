## KCALS.NET ##
###############
<IfModule mod_mime.c>
	Options -Indexes
	SetEnv TZ America/Sao_Paulo
	AddDefaultCharset UTF-8
	AddType application/font-woff .woff
	AddType font/ttf .ttf
	AddType application/x-web-app-manifest+json .webapp
	AddType text/cache-manifest .manifest
	AddType text/cache-manifest .appcache
	AddType application/octet-stream .bin
	AddType application/octet-stream .img
	AddType text/plain .db .txt .md .sql
</IfModule>

<IfModule mod_expires.c>
	ExpiresActive On
	ExpiresDefault now
</IfModule>

<IfModule mod_headers.c>
	Header add Access-Control-Allow-Origin "*"
	Header set Cache-Control "max-age=0"
	Header unset ETag
	FileETag None
</IfModule>

<IfModule mod_deflate.c>
	AddOutputFilterByType DEFLATE text/html text/plain text/xml text/css text/javascript application/javascript font/ttf application/font-woff text/cache-manifest application/x-web-app-manifest+json application/octet-stream
</IfModule>

